{
  "compilerOptions": {
    "target": "es2022",
    "lib": ["es2017", "dom"],
    "module": "esnext",
    "moduleResolution": "node",
    "baseUrl": "./",
    "outDir": "workflow",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "skipLibCheck": true,
    "allowJs": true
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
